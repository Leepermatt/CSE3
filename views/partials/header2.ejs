<header id="top-header">
    <span class="siteName">
        <a href="/" title="Return to home page"> CSE Motors</a>
    </span>
    <div id="tools">


        <% if (userLoggedIn) { %>
            <!-- Show "Welcome [Username]" and "Logout" link when the user is logged in -->
            <span>Welcome <%= userName %></span>
            <a title="Click to manage your account" href="/account/manage">Account Management</a>
            <a title="Click to log out" href="/account/logout">Logout</a>
            <% } else { %>
                <!-- Show "My Account" link when the user is not logged in -->
                <a title="Click to log in" href="/account/login">My Account</a>
                <li><a href="account/register">Register</a></li>
                <% } %>



